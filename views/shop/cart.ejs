<%- include('../includes/head.ejs') %>
  </head>

  <body>
    <%- include('../includes/navigation.ejs') %>
    <main>
      <% if (cartItems.length > 0) { %>
        <ul>
          <% cartItems.forEach((item) => { %>
            <li><%= item.productData.title %> (<%= item.qty %>)</li>
          <% }) %>
        </ul>

      <% } else { %>
        <h2>No items in the cart</h2>
      <% } %>
    </main>
    <%- include('../includes/end.ejs') %>